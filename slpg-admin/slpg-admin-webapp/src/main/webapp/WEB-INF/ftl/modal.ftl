<div class="am-modal am-modal-alert" tabindex="-1" id="my-alert">
    <div class="am-modal-dialog">
        <div class="am-modal-hd"><span id="my-alert-title"></span></div>
        <div class="am-modal-bd">
            <span id="my-alert-content"></span>
        </div>
        <div class="am-modal-footer">
            <span class="am-modal-btn" id="my-alert-btn">确定</span>
        </div>
    </div>
</div>
<div class="am-modal am-modal-confirm" tabindex="-1" id="my-confirm">
    <div class="am-modal-dialog">
        <div class="am-modal-hd"><span id="my-confirm-title"></span></div>
        <div class="am-modal-bd">
            <span id="my-confirm-content"></span>
        </div>
        <div class="am-modal-footer">
            <span class="am-modal-btn" data-am-modal-cancel>取消</span>
            <span class="am-modal-btn" data-am-modal-confirm>确定</span>
        </div>
    </div>
</div>